@model TeploPro.Models.HomeViewModels.ProjectResultViewModel
@{
    ViewData["Title"] = "Result";
}

<section class="result-main">
    <div class="container">
        <div class="result-main__head">
            <h2 class="result-main__heading">Результаты прогноза показателей теплового режима доменной плавки</h2>
            @*<a class="result-main__back" asp-area="" asp-controller="Home" asp-action="Index">Назад</a>*@
        </div>
        <div class="result-main__data">
            <p class="result-main__data-heading">Сокращенная форма результатов</p>
            <table class="result-table">
                <tr id="thead">
                    <th colspan="3">Параметр</th>
                    <th scope="col">Значение (базовый пер.)</th>
                    <th scope="col">Значение (проектный пер.)</th>
                    <th scope="col">Отклонение</th>
                    <th scope="col">Ед. изм.</th>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Индекс низа доменной печи</td>
                    <td>
                        <span class="result-main__data-value" title="Копировать значение">@Math.Round(Model.BaseResult.Result.IndexOfTheBottomOfTheFurnace, 2)</span>
                    </td>
                    <td>
                        <span class="result-main__data-value" title="Копировать значение">@Math.Round(Model.ProjectResult.Result.IndexOfTheBottomOfTheFurnace, 2)</span>
                    </td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Result.IndexOfTheBottomOfTheFurnace - Model.ProjectResult.Result.IndexOfTheBottomOfTheFurnace), 2)</td>
                    <td></td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Индекс верха доменной печи</td>
                    <td>
                        <span class="result-main__data-value" title="Копировать значение">@Math.Round(Model.BaseResult.Result.IndexOfTheFurnaceTop, 2)</span>
                    </td>
                    <td>
                        <span class="result-main__data-value" title="Копировать значение">@Math.Round(Model.ProjectResult.Result.IndexOfTheFurnaceTop, 2)</span>
                    </td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Result.IndexOfTheFurnaceTop - Model.ProjectResult.Result.IndexOfTheFurnaceTop), 2)</td>
                    <td></td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Теоретическая температура горения</td>
                    <td>
                        <span class="result-main__data-value" title="Копировать значение">@Math.Round(Model.BaseResult.Result.TheoreticalBurningTemperatureOfCarbonCoke, 0)</span>
                    </td>
                    <td>
                        <span class="result-main__data-value" title="Копировать значение">@Math.Round(Model.ProjectResult.Result.TheoreticalBurningTemperatureOfCarbonCoke, 0)</span>
                    </td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Result.TheoreticalBurningTemperatureOfCarbonCoke - Model.ProjectResult.Result.TheoreticalBurningTemperatureOfCarbonCoke), 0)</td>
                    <td>°С</td>
                </tr>
            </table>
        </div>
        <div class="result-main__checkbox checkbox-content">
            <input id="showFullResult" type="checkbox" autocomplete="off">
            <label for="showFullResult">Показать полную форму результатов</label>
        </div>
        <div class="result-main__table" style="display: none">
            <p class="result-main__table-heading">Полная форма результатов</p>
            <table id="resultTable" class="result-table">
                @* ИСХОДНЫЕ ДАННЫЕ *@
                <tr>
                    <td colspan="7" style="text-align: center; background-color: #ffd05b;">ИСХОДНЫЕ ДАННЫЕ</td>
                </tr>
                <tr id="thead">
                    <th colspan="3">Параметр</th>
                    <th scope="col">Значение (базовый)</th>
                    <th scope="col">Значение (прогноз)</th>
                    <th scope="col">Отклонение</th>
                    <th scope="col">Ед. изм.</th>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Номер доменной печи</td>
                    <td>@Model.BaseResult.Input.NumberOfFurnace</td>
                    <td>@Model.ProjectResult.Input.NumberOfFurnace</td>
                    <td>@Math.Abs(Model.BaseResult.Input.NumberOfFurnace - Model.ProjectResult.Input.NumberOfFurnace)</td>
                    <td></td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Полезный объем печи</td>
                    <td>@Model.BaseResult.Input.UsefulVolumeOfFurnace</td>
                    <td>@Model.ProjectResult.Input.UsefulVolumeOfFurnace</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.UsefulVolumeOfFurnace - Model.ProjectResult.Input.UsefulVolumeOfFurnace), 4)</td>
                    <td>м<sup>3</sup></td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Полезная высота печи</td>
                    <td>@Model.BaseResult.Input.UsefulHeightOfFurnace</td>
                    <td>@Model.ProjectResult.Input.UsefulHeightOfFurnace</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.UsefulHeightOfFurnace - Model.ProjectResult.Input.UsefulHeightOfFurnace), 4)</td>
                    <td>мм</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Диаметр колошника</td>
                    <td>@Model.BaseResult.Input.DiameterOfColoshnik</td>
                    <td>@Model.ProjectResult.Input.DiameterOfColoshnik</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.DiameterOfColoshnik - Model.ProjectResult.Input.DiameterOfColoshnik), 4)</td>
                    <td>мм</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Диаметр распара</td>
                    <td>@Model.BaseResult.Input.DiameterOfRaspar</td>
                    <td>@Model.ProjectResult.Input.DiameterOfRaspar</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.DiameterOfRaspar - Model.ProjectResult.Input.DiameterOfRaspar), 4)</td>
                    <td>мм</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Диаметр горна</td>
                    <td>@Model.BaseResult.Input.DiameterOfHorn</td>
                    <td>@Model.ProjectResult.Input.DiameterOfHorn</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.DiameterOfHorn - Model.ProjectResult.Input.DiameterOfHorn), 4)</td>
                    <td>мм</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Высота горна</td>
                    <td>@Model.BaseResult.Input.HeightOfHorn</td>
                    <td>@Model.ProjectResult.Input.HeightOfHorn</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.HeightOfHorn - Model.ProjectResult.Input.HeightOfHorn), 4)</td>
                    <td>мм</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Высота фурм</td>
                    <td>@Model.BaseResult.Input.HeightOfTuyeres</td>
                    <td>@Model.ProjectResult.Input.HeightOfTuyeres</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.HeightOfTuyeres - Model.ProjectResult.Input.HeightOfTuyeres), 4)</td>
                    <td>мм</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Высота заплечников</td>
                    <td>@Model.BaseResult.Input.HeightOfZaplechiks</td>
                    <td>@Model.ProjectResult.Input.HeightOfZaplechiks</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.HeightOfZaplechiks - Model.ProjectResult.Input.HeightOfZaplechiks), 4)</td>
                    <td>мм</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Высота распара</td>
                    <td>@Model.BaseResult.Input.HeightOfRaspar</td>
                    <td>@Model.ProjectResult.Input.HeightOfRaspar</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.HeightOfRaspar - Model.ProjectResult.Input.HeightOfRaspar), 4)</td>
                    <td>мм</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Высота шахты</td>
                    <td>@Model.BaseResult.Input.HeightOfShaft</td>
                    <td>@Model.ProjectResult.Input.HeightOfShaft</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.HeightOfShaft - Model.ProjectResult.Input.HeightOfShaft), 4)</td>
                    <td>мм</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Высота колошника</td>
                    <td>@Model.BaseResult.Input.HeightOfColoshnik</td>
                    <td>@Model.ProjectResult.Input.HeightOfColoshnik</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.HeightOfColoshnik - Model.ProjectResult.Input.HeightOfColoshnik), 4)</td>
                    <td>мм</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Установленный уровень насыпи</td>
                    <td>@Model.BaseResult.Input.EstablishedLevelOfEmbankment</td>
                    <td>@Model.ProjectResult.Input.EstablishedLevelOfEmbankment</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.EstablishedLevelOfEmbankment - Model.ProjectResult.Input.EstablishedLevelOfEmbankment), 4)</td>
                    <td>мм</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Число фурм</td>
                    <td>@Model.BaseResult.Input.NumberOfTuyeres</td>
                    <td>@Model.ProjectResult.Input.NumberOfTuyeres</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.NumberOfTuyeres - Model.ProjectResult.Input.NumberOfTuyeres), 4)</td>
