                <tr>
                    <td colspan="3" scope="row">Удельный выход шлака (по данным тех. отчета)</td>
                    <td>@Model.BaseResult.Input.SpecificSlagYield</td>
                    <td>@Model.ProjectResult.Input.SpecificSlagYield</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.SpecificSlagYield - Model.ProjectResult.Input.SpecificSlagYield), 4)</td>
                    <td>кг/т чугуна</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Теплоёмкость агломерата</td>
                    <td>@Model.BaseResult.Input.HeatCapacityOfAgglomerate</td>
                    <td>@Model.ProjectResult.Input.HeatCapacityOfAgglomerate</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.HeatCapacityOfAgglomerate - Model.ProjectResult.Input.HeatCapacityOfAgglomerate), 4)</td>
                    <td>кДж/(кг &#x2219 °С)</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Теплоёмкость окатышей</td>
                    <td>@Model.BaseResult.Input.HeatCapacityOfPellets</td>
                    <td>@Model.ProjectResult.Input.HeatCapacityOfPellets</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.HeatCapacityOfPellets - Model.ProjectResult.Input.HeatCapacityOfPellets), 4)</td>
                    <td>кДж/(кг &#x2219 °С)</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Теплоёмкость кокса</td>
                    <td>@Model.BaseResult.Input.HeatCapacityOfCoke</td>
                    <td>@Model.ProjectResult.Input.HeatCapacityOfCoke</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.HeatCapacityOfCoke - Model.ProjectResult.Input.HeatCapacityOfCoke), 4)</td>
                    <td>кДж/(кг &#x2219 °С)</td>
                </tr>
                <tr>
                    <td colspan="3" scope="row">Принятое значение температуры резервной зоны</td>
                    <td>@Model.BaseResult.Input.AcceptedTemperatureOfBackupZone</td>
                    <td>@Model.ProjectResult.Input.AcceptedTemperatureOfBackupZone</td>
                    <td>@Math.Round(Math.Abs(Model.BaseResult.Input.AcceptedTemperatureOfBackupZone - Model.ProjectResult.Input.AcceptedTemperatureOfBackupZone), 4)</td>
                    <td>°С</td>
                </tr>
